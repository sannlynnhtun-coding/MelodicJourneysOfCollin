@page "/albums"
<div class="row">
    <div class="col-md-6 col-sm-6">
        <div class="vertical-item slide-media text-center loop-color">
            <div class="item-media-wrap">
                <img src="assets/img/cd.png" alt="">
                <div class="item-media back-media">
                    <img src="assets/img/cd.png" alt="" width="100%" height="90%">
                </div>
                <div class="item-media">
                    <img src="assets/img/01.jpg" alt="" width="100%">
                    <div class="media-links no-overlay">
                        <div class="links-wrap">
                            <a href="single-album.html" class="p-link" @onclick="GoTo"></a>
                        </div>
                        <div class="bottom-links">
                            <a href="https://play.google.com/store/music" target="blanck">
                                <img src="assets/img/google-play.png" alt="">
                            </a>
                            <a href="https://itunes.apple.com/us/genre/music" target="blanck">
                                <img src="assets/img/app-store.png" alt="">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="item-content topmargin_25 pb-4">
                <div class="star-rating" title="Rated 4.0 out of 5">
                    <span>
                        <strong class="rating">4.0</strong> out of 5
                    </span>
                </div>
                <h4 class="entry-title topmargin_5">
